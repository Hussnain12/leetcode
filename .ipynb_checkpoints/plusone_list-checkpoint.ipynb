   "source": [
    "def plusOne(digits):\n",
    "    string_dig=''\n",
    "    new_list=[]\n",
    "    for i in digits:\n",
    "        string_dig+=str(i)\n",
    "    string_dig=int(string_dig)+1\n",
    "    for i in str(string_dig):\n",
    "        new_list.append(int(i))\n",
    "    return new_list\n",
    "plusOne([99])\n"
   ]
